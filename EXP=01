#include <stdio.h>

int main() {
    float a, b, c, d, e;
    float sum, avg;

    // Input marks
    printf("Enter Marks (0-100): ");
    scanf("%f", &a);
    printf("Enter Marks (0-100): ");
    scanf("%f", &b);
    printf("Enter Marks (0-100): ");
    scanf("%f", &c);
    printf("Enter Marks (0-100): ");
    scanf("%f", &d);
    printf("Enter Marks (0-100): ");
    scanf("%f", &e);

    // Validate marks
    if (a < 0 || a > 100 ||
        b < 0 || b > 100 ||
        c < 0 || c > 100 ||
        d < 0 || d > 100 ||
        e < 0 || e > 100) {
        printf("Invalid marks entered. Please re-enter marks within the range 0-100.\n");
        return 1; // Exit the program with an error code
    }

    // Calculate sum and average
    sum = a + b + c + d + e;
    avg = sum / 5;

    // Output average and result
    printf("Average = %.2f\n", avg);
    if (avg >= 40) {
        printf("Passed\n");
    } else {
        printf("Failed\n");
    }

    return 0;
}
